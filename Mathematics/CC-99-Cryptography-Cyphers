    "from IPython.display import display\n",
    "button = w.Button(button_style='info',description=\"View Decoded Message\", layout=Layout(width='20%', height='30px'))\n",
    "display(button)\n",
    "\n",
    "def on_button_clicked(b):\n",
    "    \n",
    "    display(Markdown(\"# Plain text message: \"))\n",
